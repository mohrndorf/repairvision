-->

<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated graphpattern -->
      <factory
            uri="http://www.sidiff.org/graphpattern/1.0"
            class="org.sidiff.graphpattern.provider.GraphpatternItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource
               org.eclipse.emf.edit.provider.IItemColorProvider"/>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.sidiff.graphpattern.category"
            name="Graph Pattern">
      </category>
   </extension>

</plugin>
