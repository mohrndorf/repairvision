<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated graphpattern -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.sidiff.graphpattern.presentation.GraphpatternModelWizardID"
            name="%_UI_GraphpatternModelWizard_label"
            class="org.sidiff.graphpattern.presentation.GraphpatternModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/GraphpatternModelFile.gif">
         <description>%_UI_GraphpatternModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated graphpattern -->
      <editor
            id="org.sidiff.graphpattern.presentation.GraphpatternEditorID"
            name="%_UI_GraphpatternEditor_label"
            icon="icons/full/obj16/GraphpatternModelFile.gif"
            extensions="graphpattern"
            class="org.sidiff.graphpattern.presentation.GraphpatternEditor"
            contributorClass="org.sidiff.graphpattern.presentation.GraphpatternActionBarContributor">
      </editor>
   </extension>
</plugin>
