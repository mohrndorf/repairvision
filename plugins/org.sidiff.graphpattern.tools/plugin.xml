<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <menu
               id="org.sidiff.graphpattern.tools.menu"
               label="Graph Pattern">
            <command
                  commandId="org.sidiff.graphpattern.tools.commands.modeltopattern"
                  style="push">
            </command>
            <command
                  commandId="org.sidiff.graphpattern.tools.command.slicegraphpattern"
                  icon="icons/slice.gif"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.sidiff.graphpattern.presentation.GraphpatternEditorID">
         <command
               commandId="org.sidiff.graphpattern.tools.command.slicegraphpattern"
               icon="icons/slice.gif"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.sidiff.graphpattern.presentation.GraphpatternEditorID">
         <command
               commandId="org.sidiff.graphpattern.tools.command.slicegraphpattern"
               icon="icons/slice.gif"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.sidiff.graphpattern.tools.ConvertModelToGraphPattern"
            commandId="org.sidiff.graphpattern.tools.commands.modeltopattern">
      </handler>
      <handler
            class="org.sidiff.graphpattern.tools.SliceGraphPattern"
            commandId="org.sidiff.graphpattern.tools.command.slicegraphpattern">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.sidiff.graphpattern.tools.commands.modeltopattern"
            name="Convert Model To Graph Pattern">
      </command>
      <command
            id="org.sidiff.graphpattern.tools.command.slicegraphpattern"
            name="Slice Graph Pattern">
      </command>
   </extension>
</plugin>
