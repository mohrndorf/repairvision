<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="RunHistoryModelGenerator"
         name="RunHistoryModelGenerator"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.sidiff.repair.history.generator.run.RunHistoryModelGeneratorEcoreAndEMF">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.sidiff.repair.history.generator.handler.GenerateHistoryModelEcoreAndEMFHandler"
            commandId="org.sidiff.repair.evaluation.command.generatehistorymodel.ecore.emf">
      </handler>
      <handler
            class="org.sidiff.repair.history.generator.handler.GenerateHistoryModelUMLandFOLHandler"
            commandId="org.sidiff.repair.evaluation.command.generatehistorymodel.uml.fol">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <menu
               label="Model History">
            <command
                  commandId="org.sidiff.repair.evaluation.command.generatehistorymodel.ecore.emf"
                  style="push">
            </command>
            <command
                  commandId="org.sidiff.repair.evaluation.command.generatehistorymodel.uml.fol"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.sidiff.difference.ui.category.silift"
            description="Generates an History Model for a list of model versions"
            id="org.sidiff.repair.evaluation.command.generatehistorymodel.ecore.emf"
            name="Generate History Model (Ecore, EMF)">
      </command>
      <command
            categoryId="org.sidiff.difference.ui.category.silift"
            description="Generates an History Model for a list of model versions"
            id="org.sidiff.repair.evaluation.command.generatehistorymodel.uml.fol"
            name="Generate History Model (UML, FOL)">
      </command>
   </extension>

</plugin>
