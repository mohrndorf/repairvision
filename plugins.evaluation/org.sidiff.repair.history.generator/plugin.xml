<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.sidiff.repair.history.generator.handler.EcoreValidationEMFFolderGenerator"
            commandId="org.sidiff.repair.evaluation.command.generatehistorymodel.ecore.emf.folder">
      </handler>
      <handler
            class="org.sidiff.repair.history.generator.handler.EcoreValidationEMFRepositoryGenerator"
            commandId="org.sidiff.repair.evaluation.command.generatehistorymodel.ecore.emf.repository">
      </handler>
      <handler
            class="org.sidiff.repair.history.generator.handler.UMLValidationFOLFolderGenerator"
            commandId="org.sidiff.repair.evaluation.command.generatehistorymodel.uml.fol.folder">
      </handler>
      <handler
            class="org.sidiff.repair.history.generator.handler.PrintUniqueMessagesHandler"
            commandId="org.sidiff.repair.evaluation.command.printmessages">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <menu
               id="org.sidiff.repair.history.menu"
               label="Model History">
            <command
                  commandId="org.sidiff.repair.evaluation.command.generatehistorymodel.ecore.emf.folder"
                  style="push">
            </command>
            <command
                  commandId="org.sidiff.repair.evaluation.command.generatehistorymodel.ecore.emf.repository"
                  style="push">
            </command>
            <command
                  commandId="org.sidiff.repair.evaluation.command.generatehistorymodel.uml.fol.folder"
                  style="push">
            </command>
            <command
                  commandId="org.sidiff.repair.evaluation.command.printmessages"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.sidiff.difference.ui.category.silift"
            description="Generates an History Model for a list of model versions"
            id="org.sidiff.repair.evaluation.command.generatehistorymodel.ecore.emf.folder"
            name="Generate History (Folder, Ecore, EMF-Validation)">
      </command>
      <command
            categoryId="org.sidiff.difference.ui.category.silift"
            description="Generates an History Model for a list of model versions"
            id="org.sidiff.repair.evaluation.command.generatehistorymodel.ecore.emf.repository"
            name="Generate History (Project, Ecore, EMF-Validation)">
      </command>
      <command
            categoryId="org.sidiff.difference.ui.category.silift"
            description="Generates an History Model for a list of model versions"
            id="org.sidiff.repair.evaluation.command.generatehistorymodel.uml.fol.folder"
            name="Generate History (Folder, UML, FOL-Validation)">
      </command>
      <command
            id="org.sidiff.repair.evaluation.command.printmessages"
            name="Print Unique Validation Error Messages">
      </command>
   </extension>
</plugin>
